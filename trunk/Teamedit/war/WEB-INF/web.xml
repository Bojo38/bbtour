<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- GreetService -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>bojo.teamedit.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/teamedit/greet</url-pattern>
  </servlet-mapping>
  
  <!-- SkillTypeEdit -->
  <servlet>
    <servlet-name>skillTypeEditImpl</servlet-name>
    <servlet-class>bojo.teamedit.server.SkillTypeEditImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>skillTypeEditImpl</servlet-name>
    <url-pattern>/teamedit/skillType</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>TeamEdit.html</welcome-file>
  </welcome-file-list>

</web-app>
